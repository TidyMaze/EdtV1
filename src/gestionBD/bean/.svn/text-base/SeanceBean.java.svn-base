package gestionBD.bean;

public class SeanceBean {
	private int idFrise;
	private int idActivite;
	private int ordre;

	public SeanceBean() {
		super();
	}

	public SeanceBean(int idFrise, int idActivite) {
		super();
		this.idFrise = idFrise;
		this.idActivite = idActivite;
	}

	public int getIdFrise() {
		return idFrise;
	}

	public void setIdFrise(int idFrise) {
		this.idFrise = idFrise;
	}

	public int getIdActivite() {
		return idActivite;
	}

	public void setIdActivite(int idActivite) {
		this.idActivite = idActivite;
	}

	public int getOrdre() {
		return this.ordre;
	}

	public void setOrdre(int ordre) {
		this.ordre = ordre;
	}
	
	public String toString(){
		return this.idFrise + " " + this.idActivite;
	}

}
